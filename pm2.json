{
  "apps": [
    {
      "name": "contributor",
      "script": "sleep 3&&pdm run python app/main.py",
      "instances": 1,
      "max_restarts": 3,
      "restart_delay": 10000,
      "exp_backoff_restart_delay": 100,
      "error_file": "error.log",
      "out_file": "/dev/null",
      "log_date_format": "YYYY-MM-DD HH-mm-ss"
    },
    {
      "name": "smee_proxy",
      "script": "smee --url $PROXY_WEBHOOK_URL -t http:127.0.0.1:8888",
      "instances": 1,
      "max_restarts": 3,
      "restart_delay": 10000,
      "exp_backoff_restart_delay": 100,
      "error_file": "error.log",
      "out_file": "/dev/null",
      "log_date_format": "YYYY-MM-DD HH-mm-ss"
    }
  ]
}
